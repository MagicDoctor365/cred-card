<swiper indicator-dots="true"
  autoplay="true" interval="3000" duration="1000">
  <block wx:for="{{bannerImgUrl}}" wx:key="*this" wx:for-item="imgUrl">
    <swiper-item>
      <image src="{{imgUrl}}" class="slide-image" width="100%" height="100%" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>

<block wx:for="{{catglist}}" wx:for-item="line" wx:key="*this">
    <icons icon-list="{{line}}" icon-size="width: 120rpx; height: 120rpx;"></icons>
</block>
